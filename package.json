{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite  --mode development",
    "dev:test": "vite  --mode test",
    "dev:prod": "vite  --mode production",
    "dev:eu": "vite  --mode eu",
    "build:dev": "tsc && vite build   --force  --mode development",
    "build:test": "tsc && vite build  --force --mode test",
    "build:prod": "tsc && vite build --mode production",
    "build:eu": "tsc && vite build --mode eu",
    "lint": "eslint src --ext ts,tsx --report-u nused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tsc": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@stomp/stompjs": "^7.0.0",
    "ahooks": "^3.7.7",
    "antd": "5.5.0",
    "axios": "^1.3.6",
    "dayjs": "^1.11.7",
    "immer": "^10.0.2",
    "less": "^4.1.3",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.21",
    "precompiled-mqtt": "^4.3.14-beta",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-router": "6.10.0",
    "react-router-dom": "6.10.0",
    "swr": "^2.1.5",
    "use-immer": "^0.9.0",
    "uuid": "^9.0.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "@types/node": "18.16.0",
    "@types/react": "^18.2.6",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.2.4",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "eslint": "^8.41.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1"
  },
  "overrides": {
    "react-json-view": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
